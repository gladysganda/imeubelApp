{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "Bundler",
    "target": "es2020",
    "lib": ["ESNext", "DOM"],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["react", "react-native"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
